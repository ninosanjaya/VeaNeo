[application]

config/name="Vea Neo V0"
config/version="1.0"
run/main_scene="res://practice.tscn"
config/quit_on_go_back=false
config/features=PackedStringArray("4.2", "GL Compatibility")
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/show_image=false
boot_splash/image="res://Ninhydro_logo_background.png"
config/icon="res://EBB.png"
config/windows_native_icon="res://EBB.ico"
boot_splash/minimum_display_time=3000

[autoload]

Global="*res://Global/global.gd"
GameManager="*res://Global/GameManager.gd"
Checkpoint="*res://Global/Checkpoint.gd"
